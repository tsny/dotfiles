    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.overrideCopy": false,
    "vim.foldfix": true,
    "vim.easymotionMarkerForegroundColorOneChar": "white",
    "vim.easymotionMarkerBackgroundColor": "black",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinitionAside"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "v"
            ],
            "after": [
                "<esc>"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "f"
            ],
            "commands": [
                "go.test.cursor"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "workbench.action.debug.start"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "p"
            ],
            "commands": [
                "go.test.package"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                ":q"
            ]
        },
        {
            "before": [
                "<leader>",
                "v",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "o",
                "<Esc>",
                "k"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "v"
            ],
            "after": [
                "V"
            ]
        },
        {
            "before": [
                "V"
            ],
            "after": [
                "v"
            ]
        },
        {
            "before": [
                "f"
            ],
            "after": [
                "<leader>",
                "<leader>",
                "s"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ],
            "when": [
                "editor.hasFocus"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "v"
            ],
            "commands": [
                "workbench.action.openSettingsJson"
            ]
        },
        {
            "before": [
                "0"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "$"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "n"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ],
            "when": [
                "editor.hasFocus"
            ]
        },
        {
            "before": [
                "C-h"
            ],
            "commands": [
                "workbench.action.navigateLeft"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ],
            "when": [
                "editor.hasFocus"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ],
            "when": [
                "editor.hasFocus"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands":[ "editor.action.showHover"]
        },
        {
            "before": [
                "C-j"
            ],
            "commands": [
                "workbench.action.navigateDown"
            ],
            "when": [
                "!terminalFocus"
            ]
        },
        {
            "before": [
                "C-k"
            ],
            "commands": [
                "workbench.action.navigateUp"
            ],
            "when": [
                "!terminalFocus"
            ]
        },
        {
            "before": [
                "C-l"
            ],
            "commands": [
                "workbench.action.navigateRight"
            ]
        }
    ],
